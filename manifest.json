{
    "manifest_version": 2,
    "name": "Google Sign-in Popup Blocker",
    "version": "1.0.2",
    "description": "Disables the \"Sign in with Google\" popup.",
    "icons": {
      "16": "icons/extension_logo.svg",
      "48": "icons/extension_logo.svg",
      "96": "icons/extension_logo.svg",
      "128": "icons/extension_logo.svg"
    },
    "background": {
      "scripts": ["google_signin_remover.js"]
    },
    "permissions": [
      "<all_urls>",
      "webRequest",
      "webRequestBlocking"
    ]
  }
  